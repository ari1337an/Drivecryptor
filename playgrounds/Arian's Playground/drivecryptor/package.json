    "@react-navigation/native-stack": "^6.9.1",
    "@regulaforensics/react-native-face-api": "4.1.1",
    "@robinbobin/react-native-google-drive-api-wrapper": "^1.2.3",
    "crypto-js": "^4.1.1",
    "nativewind": "^2.0.11",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-background-actions": "^2.6.7",
    "react-native-blob-util": "^0.16.4",
    "react-native-document-picker": "^8.1.2",
    "react-native-file-viewer": "^2.1.5",
